{
  "version": 2,
  "builds": [
    {
      "src": "./next.config.mjs",
      "use": "@vercel/static-build"
    },
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "./index.js"
    },
    {
      "src": "/(.*)",
      "dest": "./next.config.mjs"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ]
}