{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": {
        "buildCommand": "yarn build",
        "installCommand": "yarn install"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "index.js",
      "methods": ["GET", "POST", "PATCH", "DELETE", "OPTIONS"]
    },
    {
      "src": "/(.*)",
      "dest": "index.js",
      "methods": ["GET", "POST", "PATCH", "DELETE", "OPTIONS"]
    }
  ]
}